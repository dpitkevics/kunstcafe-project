    {% extends '::admin_layout.html.twig' %}

{% block content %}
    <div class="row">
        <h1>Artist</h1>
    </div>

    <div class="row">
        <div class="large-12 small-12 columns">
            <table class="record_properties">
                <tbody>
                    <tr>
                        <th>Id</th>
                        <td>{{ entity.id }}</td>
                    </tr>
                    <tr>
                        <th>Firstname</th>
                        <td>{{ entity.firstName }}</td>
                    </tr>
                    <tr>
                        <th>Lastname</th>
                        <td>{{ entity.lastName }}</td>
                    </tr>
                    <tr>
                        <th>Quote</th>
                        <td>{{ entity.quote }}</td>
                    </tr>
                    <tr>
                        <th>About</th>
                        <td>{{ entity.about }}</td>
                    </tr>
                </tbody>
            </table>
        </div>
    </div>

        <div class="row">
    <div class="large-12 small-12 columns">
        <ul class="button-group">
            <li>
                <a href="{{ path('kunstcafe_main_artist_crud') }}" class="button">
                    Back to the list
                </a>
            </li>
                        <li>
                <a href="{{ path('kunstcafe_main_artist_crud_edit', { 'id': entity.id }) }}" class="button">
                    Edit
                </a>
            </li>
                                    <li>{{ form(delete_form) }}</li>
                    </ul>
    </div>
</div>
{% endblock %}
